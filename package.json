{
  "name": "rocket-cloud-website",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "old:start": "ng serve",
    "old:build": "ng build",
    "start": "npm run serve",
    "serve": "npx concurrently \"npm run serve:server\" \"npm run serve:client\"",
    "serve:client": "ng serve --proxy-config ./proxy.config.js",
    "serve:server": "npx ts-node-dev --respawn --project ./src/server/tsconfig.server.json --inspect -- ./src/server/server.ts",
    "dist": "npm run dist:clean && npx concurrently \"npm run dist:server\" \"npm run dist:client\" \"npm run dist:copyArtifacts\" \"npm run dist:copyPackageJson\"",
    "dist:client": "ng build --prod",
    "dist:server": "npx tsc --project ./src/server/tsconfig.server.json --outDir ./dist/server --sourceMap false",
    "dist:clean": "npx rimraf ./dist",
    "dist:copyArtifacts": "npx cpx ./artifacts/**/* ./dist",
    "dist:copyPackageJson": "npx cpx ./package.json ./dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "body-parser": "^1.18.3",
    "core-js": "^2.5.4",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-request-id": "^1.4.1",
    "mongoose": "^5.5.0",
    "morgan": "^1.9.1",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "winston": "^3.2.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.8",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/mongoose": "^5.3.26",
    "@types/morgan": "^1.7.35",
    "@types/node": "~8.9.4",
    "@types/winston": "^2.4.4",
    "concurrently": "^4.1.0",
    "cpx": "^1.5.0",
    "rimraf": "^2.6.3",
    "ts-node": "^7.0.1",
    "ts-node-dev": "^1.0.0-pre.32",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
